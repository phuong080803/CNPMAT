<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container_settings"
    android:layout_width="match_parent"
    android:layout_height="926dp"

    android:gravity="left|top"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/img_shape"
        android:layout_width="290dp"
        android:layout_height="270dp"
        android:layout_gravity="top|left"
        android:layout_marginLeft="-89dp"
        android:layout_marginTop="-117dp"
        android:scaleType="centerInside"
        android:src="@drawable/img_shape" />

    <LinearLayout
        android:id="@+id/container_auto_layout_vertical"
        android:layout_width="428dp"
        android:layout_height="0dp"
        android:layout_gravity="center_horizontal"
        android:layout_weight="1"

        android:gravity="center_horizontal|top"
        android:layout_marginTop="-120dp"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/container_auto_layout_horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"
            android:layout_marginTop="24dp"
            android:clipToPadding="false"
            android:gravity="left|center_vertical"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/container_auto_layout_horizontal1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:gravity="center"
                android:orientation="horizontal"
                android:paddingBottom="8.18dp"
                android:paddingLeft="8.18dp"
                android:paddingRight="8.18dp"
                android:paddingTop="8.18dp">

                <ImageView
                    android:id="@+id/container_navigation_arrow_back_ios"
                    android:layout_width="19.64dp"
                    android:layout_height="19.64dp"
                    android:scaleType="centerInside"
                    android:src="@drawable/setting" />
            </LinearLayout>

            <TextView
                android:id="@+id/text_settings"
                android:layout_width="249dp"
                android:layout_height="33dp"
                android:layout_marginLeft="25dp"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Settings"
                android:textColor="#FF000000"
                android:textSize="26dp" />
        </LinearLayout>

        <FrameLayout
            android:id="@+id/container_group_Notifications"
            android:layout_width="364dp"
            android:layout_height="85dp"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"
            android:layout_marginTop="20dp"
            android:layout_gravity="center_horizontal">

            <ImageView
                android:id="@+id/image_rectangle1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/image_rectangle"
                android:scaleType="centerCrop" />

            <TextView
                android:id="@+id/text_notifications"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_marginLeft="100dp"
                android:layout_marginTop="25dp"
                android:layout_gravity="top|left"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Notifications"
                android:textColor="#FF2C3A4B"
                android:textSize="20dp" />

            <FrameLayout
                android:id="@+id/image_group"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="10dp"
                android:layout_gravity="top|left">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/drawable_shape">


                </FrameLayout>

                >
            </FrameLayout>

            <ImageView
                android:id="@+id/container_line"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="18dp"
                android:layout_marginRight="17.99dp"
                android:layout_marginTop="85dp"
                android:scaleType="centerCrop"
                android:src="@drawable/container_line" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/container_group_Security"
            android:layout_width="364dp"
            android:layout_height="85dp"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"
            android:layout_marginTop="20dp"
            android:layout_gravity="center_horizontal">

            <FrameLayout
                android:id="@+id/container_group2"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/image_rectangle2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/image_rectangle"
                    android:scaleType="centerCrop" />


                <TextView
                    android:id="@+id/text_security"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginLeft="100dp"
                    android:layout_marginTop="25dp"
                    android:layout_gravity="top|left"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Security"
                    android:textColor="#FF2C3A4B"
                    android:textSize="20dp" />


            </FrameLayout>

            <FrameLayout
                android:id="@+id/image_group1"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="12dp"
                android:layout_gravity="top|left">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/drawable_shape_2">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/drawable_shape_3">


                    </FrameLayout>
                </FrameLayout>


            </FrameLayout>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/container_group_Appearance"
            android:layout_width="364dp"
            android:layout_height="85dp"
            android:layout_marginLeft="24dp"
            android:layout_marginRight="24dp"
            android:layout_marginTop="20dp"
            android:layout_gravity="center_horizontal">

            <FrameLayout
                android:id="@+id/container_group4"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/image_rectangle5"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/image_rectangle"
                    android:scaleType="centerCrop" />

                <TextView
                    android:id="@+id/text_appearance"
                    android:layout_width="wrap_content"
                    android:layout_height="30dp"
                    android:layout_marginLeft="100dp"
                    android:layout_marginTop="25dp"
                    android:layout_gravity="top|left"
                    android:gravity="left|center_vertical"
                    android:includeFontPadding="false"
                    android:maxLines="1"
                    android:singleLine="true"
                    android:text="Appearance"
                    android:textColor="#FF2C3A4B"
                    android:textSize="20dp" />

                <ImageView
                    android:id="@+id/container_line2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="18dp"
                    android:layout_marginRight="17.99dp"
                    android:layout_marginTop="85dp"
                    android:scaleType="centerCrop"></ImageView>

            </FrameLayout>

            <FrameLayout
                android:id="@+id/image_group2"
                android:layout_width="55dp"
                android:layout_height="55dp"
                android:layout_marginLeft="17dp"
                android:layout_marginTop="12dp"
                android:layout_gravity="top|left">



                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/drawable_shape_6">


                    </FrameLayout>
                </FrameLayout>


            </FrameLayout>
        <FrameLayout
        android:id="@+id/container_group_help"
        android:layout_width="364dp"
        android:layout_height="85dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginTop="20dp"
        android:layout_gravity="center_horizontal">
            <ImageView
                android:id="@+id/image_rectangle3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/image_rectangle"
                android:scaleType="centerCrop" />
        <FrameLayout
            android:id="@+id/container_group9"
            android:layout_width="match_parent"
            android:layout_height="match_parent">



            <TextView
                android:id="@+id/text_help"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="top|left"
                android:layout_marginLeft="100dp"
                android:layout_marginTop="25dp"
                android:gravity="left|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Help"
                android:textColor="#FF2C3A4B"
                android:textSize="20dp" />

            >
        </FrameLayout>

        <FrameLayout
            android:id="@+id/image_group3"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_marginLeft="17dp"
            android:layout_marginTop="12dp"
            android:layout_gravity="top|left">



            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/drawable_shape_7">


            </FrameLayout>
        </FrameLayout>


    </FrameLayout>




        <!-- 227:4330:space-view0 Auto Layout Vertical_space_view0 space-view -->

    </LinearLayout>
    <LinearLayout
        android:id="@+id/container_frame"
        android:layout_width="match_parent"
        android:layout_height="73dp"
        android:layout_gravity="bottom"
        android:layout_marginBottom="25dp"
        android:background="@drawable/container_frame"
        android:orientation="horizontal"
        android:gravity="center_horizontal"
        android:padding="10dp">

        <!-- Home Section -->
        <FrameLayout
            android:id="@+id/container_group3"
            android:layout_width="80dp"
            android:layout_height="45dp"
            android:layout_marginTop="10dp">
            <ImageView
                android:id="@+id/image_home"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:src="@drawable/image_home" />
        </FrameLayout>

        <!-- Profile Section -->
        <FrameLayout
            android:id="@+id/container_group6"
            android:layout_width="80dp"
            android:layout_height="45dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp">
            <ImageView
                android:id="@+id/image_account_circle"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:src="@drawable/image_account_circle" />
        </FrameLayout>

        <!-- Settings Section -->


        <!-- Applications Section -->
        <FrameLayout
            android:id="@+id/container_group10"
            android:layout_width="80dp"
            android:layout_height="45dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp">
            <ImageView
                android:id="@+id/image_list"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:src="@drawable/image_list" />
        </FrameLayout>
        <FrameLayout
            android:id="@+id/container_group5"
            android:layout_width="80dp"
            android:layout_height="45dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp">
            <ImageView
                android:id="@+id/setting"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:src="@drawable/setting" />
        </FrameLayout>
    </LinearLayout>
</LinearLayout>
